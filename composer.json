{
  "name": "ergebnis/http-method",
  "description": "Provides constants for HTTP request methods.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "http",
    "request",
    "method"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/http-method",
  "support": {
    "issues": "https://github.com/ergebnis/http-method/issues",
    "source": "https://github.com/ergebnis/http-method"
  },
  "require": {
    "php": "^7.4 || ^8.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.28.1",
    "ergebnis/license": "^1.2.0",
    "ergebnis/php-cs-fixer-config": "^4.4.0",
    "phpunit/phpunit": "^9.5.21",
    "psalm/plugin-phpunit": "~0.17.0",
    "vimeo/psalm": "^4.23"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Http\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Http\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true
    },
    "platform": {
      "php": "7.4.26"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
