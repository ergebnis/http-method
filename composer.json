{
  "name": "ergebnis/http-method",
  "type": "library",
  "description": "Provides constants for HTTP request methods.",
  "keywords": [
    "http",
    "request",
    "method"
  ],
  "homepage": "https://github.com/ergebnis/http-method",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.11.0",
    "ergebnis/license": "^1.1.0",
    "ergebnis/php-cs-fixer-config": "^2.10.0",
    "ergebnis/phpstan-rules": "~0.15.3",
    "ergebnis/test-util": "^1.4.0",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "~0.12.53",
    "phpstan/phpstan-deprecation-rules": "~0.12.6",
    "phpstan/phpstan-phpunit": "~0.12.17",
    "phpstan/phpstan-strict-rules": "~0.12.7",
    "phpunit/phpunit": "^8.5.13",
    "psalm/plugin-phpunit": "~0.12.2",
    "vimeo/psalm": "^3.18"
  },
  "config": {
    "platform": {
      "php": "7.2.25"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Http\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Http\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/ergebnis/http-method/issues",
    "source": "https://github.com/ergebnis/http-method"
  }
}
