{
  "name": "ergebnis/http-method",
  "description": "Provides constants for HTTP request methods.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "http",
    "request",
    "method"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/http-method",
  "support": {
    "issues": "https://github.com/ergebnis/http-method/issues",
    "source": "https://github.com/ergebnis/http-method"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.36.0",
    "ergebnis/license": "^2.2.0",
    "ergebnis/php-cs-fixer-config": "^5.15.1",
    "ergebnis/phpunit-slow-test-detector": "^2.3.0",
    "phpunit/phpunit": "^10.3.4",
    "psalm/plugin-phpunit": "~0.18.4",
    "rector/rector": "~0.18.3",
    "vimeo/psalm": "^5.15.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Http\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Http\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "8.1.21"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
