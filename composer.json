{
  "name": "ergebnis/http-method",
  "type": "library",
  "description": "Provides constants for HTTP request methods.",
  "keywords": [
    "http",
    "request",
    "method"
  ],
  "homepage": "https://github.com/ergebnis/http-method",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.0.0",
    "ergebnis/php-cs-fixer-config": "~1.1.1",
    "ergebnis/phpstan-rules": "~0.14.1",
    "ergebnis/test-util": "~0.9.0",
    "phpstan/extension-installer": "^1.0.3",
    "phpstan/phpstan": "~0.11.19",
    "phpstan/phpstan-deprecation-rules": "~0.11.2",
    "phpstan/phpstan-strict-rules": "~0.11.1",
    "phpunit/phpunit": "^8.5.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Http\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Http\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/ergebnis/http-method/issues",
    "source": "https://github.com/ergebnis/http-method"
  }
}
