  "keywords": [
    "http",
    "request",
    "method"
  ],
  "homepage": "https://github.com/ergebnis/http-method",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.3.2",
    "ergebnis/license": "~0.1.0",
